{
  "name": "mat-lang-syntax",
  "displayName": "MAT Syntax",
  "description": "Syntax highlighting for the MAT programming language",
  "version": "0.0.1",
  "publisher": "your-publisher-name", 
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mat",
        "aliases": ["MAT", "mat"],
        "extensions": [".mat"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mat",
        "scopeName": "source.mat",
        "path": "./syntaxes/mat.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "MAT-Lang Syntax Highlighting",
        "uiTheme": "vs-dark",
        "path": "./themes/mat-lang-color-theme.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://https://github.com/HardBoss07/mat-lang-vsc-extension"
  },
  "icon": "icon.png"
}